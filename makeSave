CC = gcc
OBJ = GestionFichier.o liste_ptr.o
PROG = Saves
CFLAGS = -Wall -g

${PROG} : ${OBJ}
	${CC} ${CFLAGS} ${OBJ} -o ${PROG}
liste_ptr.o : liste_ptr.c
	${CC} ${CFLAGS} -c liste_ptr.c
GestionFichier.o : GestionFichier.c liste_ptr.h
	${CC} ${CFLAGS} -c GestionFichier.c liste_ptr.h
	
#suppression des fichiers temporaires
clean :
	rm -i *.o
	rm -i *.gch
	
mrproper : clean
	rm -i prog*
